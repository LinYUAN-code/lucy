{
  "name": "lucy-compiler",
  "version": "1.0.8",
  "description": "a simple lexer , LL(1) , LR(1) npm lib. ",
  "main": "dist/index.cjs.js",
  "scripts": {
    "build": "rollup -c rollup.config.prod.js --bundleConfigAsCjs",
    "dev": "rollup -c rollup.config.dev.js --bundleConfigAsCjs",
    "release": "npm run build && npm publish",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinYUAN-code/lucy"
  },
  "keywords": [
    "compiler",
    "LL(1)",
    "LR(1)",
    "lexer"
  ],
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": "linYuan",
  "license": "ISC",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "colors": "^1.4.0",
    "eslint": "^8.31.0",
    "jest": "^29.4.1",
    "rollup": "^3.9.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-strip-code": "^0.2.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
